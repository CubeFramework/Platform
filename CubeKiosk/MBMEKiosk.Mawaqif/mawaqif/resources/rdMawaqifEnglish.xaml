    <Color x:Key="ButtonColor1">#FF82C8D4</Color>
    <Color x:Key="ButtonColor2">White</Color>
    <!--Buttons text color-->
    <Color x:Key="ButtonForeground">#FF59615E</Color>

    <!--Text Color-->
    <SolidColorBrush x:Key="TextForeground"
                     Color="#FF59615E" />

    <!--Message text color-->
    <Color x:Key="MessageForeground">White</Color>

