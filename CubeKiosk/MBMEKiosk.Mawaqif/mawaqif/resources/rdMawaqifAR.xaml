    <Color x:Key="ContentBackgroundBrush1">#FF92DDED</Color>
    <Color x:Key="ContentBackgroundBrush2">#FFE7F8FB</Color>

    <!--Background for the dim layer, semi transparent layer to display text/processing screen-->
