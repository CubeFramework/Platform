    <Color x:Key="ProgressNormalBrushGradient1">#FF92DDED</Color>
    <Color x:Key="ProgressNormalBrushGradient2">#FF00B4CB</Color>
    <Color x:Key="ProgressNormalBrushGradient3">#FF00E2FF</Color>
