        			VerticalAlignment="Center" Text="{Binding Transaction.CardNo}"/>
        	</Grid>
        </StackPanel>
        <StackPanel HorizontalAlignment="Center"
                    Width="200"
                    Visibility="{Binding Transaction.CardPayment, ConverterParameter=VisibleCollapsed, Converter={StaticResource boolToVisibilityConverter}}">
            <Grid HorizontalAlignment="Center"
                  VerticalAlignment="Top">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="0.643*" />
                    <ColumnDefinition Width="0.357*" />
                </Grid.ColumnDefinitions>
                <TextBlock Text="{DynamicResource mawaqif_txt_rcpt_Merchant_ID_1}"
