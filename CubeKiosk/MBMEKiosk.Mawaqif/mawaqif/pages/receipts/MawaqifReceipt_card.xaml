        <Style x:Key="Mawaqif_Rcpt_Title_TextStyle"
               TargetType="{x:Type TextBlock}">
            <Setter Property="FontFamily"
                    Value="{DynamicResource Mawaqif_ReceiptText_Font}" />
            <Setter Property="FontSize"
                    Value="8" />
            <Setter Property="TextWrapping"
                    Value="NoWrap" />
            <Setter Property="TextAlignment"
                    Value="Center" />
            <Setter Property="FontWeight"
                    Value="Normal" />
        </Style>

        <Style x:Key="Mawaqif_Rcpt_Title_TextStyle_ar"
               TargetType="{x:Type TextBlock}">
            <Setter Property="FontFamily"
                    Value="{DynamicResource Mawaqif_ReceiptText_Font_ar}" />
            <Setter Property="FontSize"
                    Value="8" />
            <Setter Property="TextWrapping"
                    Value="NoWrap" />
            <Setter Property="TextAlignment"
                    Value="Center" />
            <Setter Property="FontWeight"
                    Value="Normal" />
        </Style>



        <Style x:Key="Mawaqif_Rcpt_Dynamic_TextStyle"
               TargetType="{x:Type TextBlock}">
            <Setter Property="FontFamily"
                    Value="{DynamicResource Mawaqif_ReceiptText_Font}" />
            <Setter Property="FontSize"
                    Value="8" />
            <Setter Property="TextWrapping"
                    Value="NoWrap" />
            <Setter Property="TextAlignment"
                    Value="Center" />
            <Setter Property="FontWeight"
                    Value="Normal" />
        </Style>


        <Style x:Key="Mawaqif_Rcpt_Dynamic_TextStyle_ar"
               TargetType="{x:Type TextBlock}">
            <Setter Property="FontFamily"
                    Value="{DynamicResource Mawaqif_ReceiptText_Font_ar}" />
            <Setter Property="FontSize"
                    Value="10" />
            <Setter Property="TextWrapping"
                    Value="NoWrap" />
            <Setter Property="TextAlignment"
                    Value="Center" />
            <Setter Property="FontWeight"
                    Value="Normal" />
        </Style>


        <Style x:Key="Mawaqif_Rcpt_Footer_TextStyle"
               TargetType="{x:Type TextBlock}">
            <Setter Property="FontFamily"
                    Value="{DynamicResource Mawaqif_ReceiptText_Font}" />
            <Setter Property="FontSize"
                    Value="8" />
            <Setter Property="TextWrapping"
                    Value="Wrap" />
            <Setter Property="TextAlignment"
                    Value="Center" />
            <Setter Property="FontWeight"
                    Value="Normal" />
        </Style>
        <Style x:Key="Mawaqif_DynamicTextBG_Style"
               TargetType="{x:Type Rectangle}">
            <Setter Property="Fill"
                    Value="White" />
            <Setter Property="Stroke"
                    Value="Black" />
            <Setter Property="StrokeThickness"
                    Value="0.5" />
        </Style>

