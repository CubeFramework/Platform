                           Grid.Column="1"
                           Margin="1.5,0,0,0" />
            </Grid>
            <Grid Height="12">
                <Rectangle Style="{DynamicResource Mawaqif_DynamicTextBG_Style}"
                           Width="200" />
                <TextBlock HorizontalAlignment="Center"
                           Style="{DynamicResource Mawaqif_Rcpt_Dynamic_TextStyle}"
                           VerticalAlignment="Center">
    				<TextBlock.Text>
    					<MultiBinding Converter="{StaticResource localisedNumberConverter}"
                                      Mode="OneWay">
    						<Binding Path="Transaction.Id" />
    						<Binding Path="Transaction.SelectedLanguageKey" />
    					</MultiBinding>
    				</TextBlock.Text>
                </TextBlock>
            </Grid>
        </StackPanel>
        <StackPanel HorizontalAlignment="Center"
        	Width="200">
        	<Grid HorizontalAlignment="Center"
